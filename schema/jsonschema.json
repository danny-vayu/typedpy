{
  "Person": {
    "description": "describes a Person with name and social security number",
    "properties": {
      "name": {
        "type": "string",
        "pattern": "'[A-Za-z]+$'",
        "maxLength": 8
      },
      "ssid": {
         "type": "string",
         "pattern": "'^\\d{3}-\\d{2}-\\d{4}$'",
         "minLength": 3
      },
      "num": {
        "type": "integer",
        "maximum": 30,
        "minimum": 10,
        "multiplesOf": 5,
        "exclusiveMaximum": false
      }
    }
  }
}
